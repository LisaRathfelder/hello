<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  
   <servlet>
    <servlet-name>notemapperServlet</servlet-name>
    <servlet-class>com.oencue.server.noteMapperImpl</servlet-class>
  </servlet>
  

  
   <servlet-mapping>
    <servlet-name>notemapperServlet</servlet-name>
    <url-pattern>/hello/notemapper</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>loginService</servlet-name>
    <servlet-class>com.oencue.server.LoginServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>loginService</servlet-name>
    <url-pattern>/hello/login</url-pattern>
  </servlet-mapping>
  
  
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Hello.html</welcome-file>
  </welcome-file-list>

</web-app>
